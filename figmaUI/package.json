{
  "name": "figma-ai-naming",
  "version": "2.0.0",
  "description": "AI-powered Figma plugin that automatically generates semantic, meaningful names for design elements. Supports single elements, batch naming, and intelligent frame structure analysis with visual understanding.",
  "keywords": [
    "figma",
    "figma-plugin",
    "ai",
    "artificial-intelligence",
    "naming",
    "automation",
    "productivity",
    "design-tools",
    "semantic-naming",
    "batch-rename"
  ],
  "author": "",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "esbuild src/code.ts --bundle --platform=browser --target=es2020 --outfile=code.js --format=iife && node scripts/build.js",
    "watch": "esbuild src/code.ts --bundle --platform=browser --target=es2020 --outfile=code.js --format=iife --watch",
    "icon": "node scripts/generate-icon.js",
    "convert-video": "node scripts/convert-video.js"
  },
  "devDependencies": {
    "@figma/plugin-typings": "^1.88.0",
    "esbuild": "^0.19.0",
    "sharp": "^0.33.5",
    "typescript": "^5.0.0"
  }
}
