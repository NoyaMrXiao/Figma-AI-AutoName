{
  "name": "figma-ai-naming",
  "version": "2.0.0",
  "description": "AI Auto Naming for Figma",
  "type": "module",
  "scripts": {
    "build": "esbuild src/code.ts --bundle --platform=browser --target=es2020 --outfile=code.js --format=iife && node scripts/build.js",
    "watch": "esbuild src/code.ts --bundle --platform=browser --target=es2020 --outfile=code.js --format=iife --watch",
    "icon": "node scripts/generate-icon.js",
    "convert-video": "node scripts/convert-video.js"
  },
  "devDependencies": {
    "@figma/plugin-typings": "^1.88.0",
    "esbuild": "^0.19.0",
    "sharp": "^0.33.5",
    "typescript": "^5.0.0"
  }
}
